{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Usuario</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nomusu</th>
                <td>{{ entity.nomUsu }}</td>
            </tr>
            <tr>
                <th>Apeusu</th>
                <td>{{ entity.apeUsu }}</td>
            </tr>
            <tr>
                <th>Fecusu</th>
                <td>{{ entity.fecUsu|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Fecult</th>
                <td>{{ entity.fecUlt|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Mailusu</th>
                <td>{{ entity.mailUsu }}</td>
            </tr>
            <tr>
                <th>Fecing</th>
                <td>{{ entity.fecIng|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ entity.password }}</td>
            </tr>
            <tr>
                <th>Salt</th>
                <td>{{ entity.salt }}</td>
            </tr>
            <tr>
                <th>Estusu</th>
                <td>{{ entity.estUsu }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('usuario') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('usuario_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
